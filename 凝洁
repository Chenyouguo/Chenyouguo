20220614
#include<stdio.h>
int main()
{
	int num1 = 0;
	int num2 = 0;
	int sum = 0;
	printf("输入俩数>");
		scanf("%d %d", &num1,&num2);
	sum = num1 + num2;
	printf("% d\n" , sum);
	return 0;

20220615
#include<stdio.h>
int main()
{
	int coding = 0;
	printf("小呆瓜输入1or0\n");
	scanf("%d", &coding);
	if (coding == 1)
	{
		printf("你直接无敌");
	}
	else
	{
		printf("你真是个小呆瓜");
	}
	return 0;
}

20220616#include<stdio.h>
int main()
{
	printf("含泪敲代码\n");
	int line = 0;
	while (line < 10000)
	{
		line++;
		printf("还得继续敲啊%d\n",line);
	}
	if(line >= 10000)
	{
	printf("你是真滴c\n");
}
	return 0;
}

