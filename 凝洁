20220621
#include<stdio.h>
struct Stu
{
	char name[20];
	int age;
	char sex[5];
	char id[15];
};
int main()
{
	struct Stu s = { "张三",20,"男","20221314" };
	printf("name=%s age=%d sex=%s id=%s\n", s.name, s.age, s.sex, s.id);
	return 0;
}

