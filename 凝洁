#include<stdio.h>
int main()
{
	int num1 = 0;
	int num2 = 0;
	int sum = 0;
	printf("输入俩数>");
		scanf("%d %d", &num1,&num2);
	sum = num1 + num2;
	printf("% d\n" , sum);
	return 0;

#include<stdio.h>
int main()
{
	int coding = 0;
	printf("小呆瓜输入1or0\n");
	scanf("%d", &coding);
	if (coding == 1)
	{
		printf("你直接无敌");
	}
	else
	{
		printf("你真是个小呆瓜");
	}
	return 0;
}
